{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/components/*": [
                "components/*"
            ],
            "@/lib/*": [
                "lib/*"
            ],
            "@/contexts/*": [
                "contexts/*"
            ],
            "@/hooks/*": [
